{
  "name": "vocabuilder",
  "main": "./lib/main",
  "version": "0.4.0",
  "description": "Let me vocabuild!",
  "keywords": [
    "translate"
  ],
  "repository": "https://github.com/t9md/atom-vocabuilder",
  "license": "MIT",
  "engines": {
    "atom": ">=1.7.0"
  },
  "devDependencies": {
    "prettier": "^1.10.2",
    "standard": "^10.0.3"
  },
  "scripts": {
    "test": "apm test",
    "watch": "fswatch -0 spec lib | xargs -0 -n1 -I{} apm test",
    "prettier": "prettier --write --no-semi --print-width=120 --no-bracket-spacing --trailing-comma=es5 'lib/**/*.js' 'spec/*.js'",
    "lint": "standard lib/*.js lib/**/*.js spec/*.js"
  },
  "activationCommands": {
    "atom-text-editor": [
      "vocabuilder:toggle-auto-search",
      "vocabuilder:manual-search"
    ]
  },
  "configSchema": {
    "useDictLink": {
      "order": 1,
      "type": "boolean",
      "default": "false"
    },
    "useSay": {
      "order": 2,
      "type": "boolean",
      "default": "false"
    },
    "useGoogle": {
      "order": 3,
      "type": "boolean",
      "default": "false"
    },
    "showCaption": {
      "order": 4,
      "type": "boolean",
      "default": "true"
    },
    "userStyle": {
      "order": 5,
      "type": "string",
      "default": "#vocabuilder-caption {\n  position: fixed;\n  top: 30%;\n  left: 5%;\n  z-index: 5;\n  width: 80%;\n  border-radius: 30px;\n  padding: 30px;\n  color: white;\n  background-color: black;\n  opacity: 0.6;\n  font-size: 120px;\n  text-align: center;\n}\n\n#vocabuilder-caption-item-2 {\n  font-size: 0.5em;\n}\n"
    }
  },
  "consumedServices": {
    "vim-mode-plus": {
      "versions": {
        "^0.1.0": "consumeVimModePlus"
      }
    }
  },
  "standard": {
    "env": {
      "atomtest": true,
      "browser": true,
      "jasmine": true,
      "node": true
    },
    "globals": [
      "atom"
    ],
    "ignore": [
      "spec/fixtures/*"
    ]
  },
  "dependencies": {
    "selenium-webdriver": "^4.0.0-alpha.1"
  }
}
